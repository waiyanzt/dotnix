# Docked profile: External monitor is primary.
profile {
    output "eDP-1" disable
    output "HDMI-A-1" enable position 0,0 mode 1920x1080@144Hz
    exec swww img /home/ztzy/Pictures/ronin.jpg
}

# Mobile profile: Laptop display is primary with correct scaling.
profile {
    output "eDP-1" enable position 0,0 
    output "HDMI-A-1" disable
    exec swww img /home/ztzy/Pictures/ronin.jpg
}
